#4

def factorial(n):
    if n == 0 or n == 1:
        return 1  # base case
    else:
        return n * factorial(n - 1)  # recursive case



#5

def hamming_distance(str1, str2):
    if len(str1) != len(str2):
        return "Strings must be of equal length"
    
    count = 0
    for i in range(len(str1)):
        if str1[i] != str2[i]:
            count += 1
    return count
